{
  "name": "advanced_react",
  "version": "1.0.0",
  "main": "lib/server.js",
  "author": "Lam Nguyen",
  "license": "MIT",
  "scripts": {
    "dev": "set NODE_PATH=./lib && nodemon --exec babel-node lib/server.js",
    "webpack": "webpack -wd",
    "test": "jest --watchAll"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      [
        "module-resolver",
        {
          "alias": {
            "@lib": "./lib"
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "webpack-cli": "^3.3.11"
  },
  "rule": {
    "linebreak-style": 0,
    "global-require": 0
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "core-js": "^2.6.11",
    "ejs": "^3.0.2",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jest": "^25.3.0",
    "nodemon": "^2.0.3",
    "pm2": "^4.2.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-context-devtool": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "webpack": "^4.42.1"
  }
}
